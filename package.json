{
  "name": "electron-webpack",
  "version": "1.0.0",
  "description": "A base Electron project configured with Webpack and Typescript",
  "main": "./dist/dev/main.js",
  "scripts": {
    "Dev:Webpack": "webpack --mode=development --config webpack.config.dev.ts --watch",
    "Dev:Electron:Start": "electron ./dist/dev/main.js",
    "Prod:Webpack": "webpack --mode=production --config webpack.config.prod.ts",
    "Prod:Electron:Start": "electron ./dist/prod/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiusnonos/electron-webpack.git"
  },
  "author": "tiusnonos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiusnonos/electron-webpack/issues"
  },
  "homepage": "https://github.com/tiusnonos/electron-webpack#readme",
  "devDependencies": {
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/node": "^13.13.5",
    "@types/tapable": "^1.0.5",
    "@types/webpack": "^4.41.12",
    "@types/webpack-merge": "^4.1.5",
    "clean-webpack-plugin": "^3.0.0",
    "electron": "^8.2.5",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "ts-loader": "^7.0.3",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  }
}
